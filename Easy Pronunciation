for t in range(int(input(""))):
    n = int(input())
    s = input()
    c = 0
    ans = False
    for i in str(s):
        if i != 'a' and i != 'e' and i != 'i' and i != 'o' and i != 'u':
            c+=1
        else:
            c=0
        if (c >= 4):
            ans = True
    if (ans == False):
        print("YES")
    else:
        print("NO")
